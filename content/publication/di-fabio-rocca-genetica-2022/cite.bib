@article{di_fabio_rocca_genetica_2022,
 abstract = {A partir del caso de la ancestría individual y poblacional en Argentina, y mediante una exploración de discursos en redes sociales y medios de comunicación, se reflexiona sobre el rol de los datos genéticos para pensar una Argentina mestiza. Nos preguntamos si el ADN puede ser una herramienta de reparación y justicia social para algunos grupos insertos en una sociedad que durante más de un siglo promovió y se percibió como homogéneamente blanca y descendiente de europeos.},
 author = {Di Fabio Rocca, Francisco and Arencibia, Valeria and Gago, Julia and Bettera Marcat, Gianina Celeste and Cardozo, Darío and Russo, María Gabriela},
 copyright = {http://creativecommons.org/licenses/by/4.0/},
 doi = {10.18800/boletindearqueologiapucp.202201.007},
 file = {Full Text:/home/gabriela/Zotero/storage/JLFRK9GA/Di Fabio Rocca et al. - 2022 - La genética como herramienta de reparación. Desafíos para una Argentina mestiza.pdf:application/pdf},
 issn = {2304-4292, 1029-2004},
 journal = {Boletín de Arqueología PUCP},
 month = {December},
 number = {31},
 pages = {139--146},
 title = {La genética como herramienta de reparación. Desafíos para una Argentina mestiza},
 url = {https://revistas.pucp.edu.pe/index.php/boletindearqueologia/article/view/26374},
 urldate = {2025-02-05},
